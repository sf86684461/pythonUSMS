iv class=\"container\" :class=\"{ 'bg-fallback': !bgImageLoaded }\" :style='containerStyle'>\r\n      <el-form :style='formStyle'>\r\n        <div v-if=\"true\" :style='{\"padding\":\"0px\",\"margin\":\"0 0 20px\",\"borderColor\":\"#ddd\",\"color\":\"#2b9afc\",\"textAlign\":\"left\",\"display\":\"inline-block\",\"background\":\"none\",\"borderWidth\":\"0px\",\"width\":\"86%\",\"lineHeight\":\"30px\",\"fontSize\":\"18px\",\"borderStyle\":\"solid\",\"fontWeight\":\"500\"}' class=\"title-container\">无人超市管理系统登录</div>\r\n        <div v-if=\"loginType==1\" class=\"list-item\" :style='{\"width\":\"100%\",\"margin\":\"0 0 10px\",\"position\":\"relative\",\"alignItems\":\"center\",\"flexWrap\":\"wrap\",\"display\":\"flex\"}'>\r\n          <div v-if=\"true\" class=\"lable\" :style='{\"color\":\"#333\",\"left\":\"-100px\",\"textAlign\":\"right\",\"width\":\"100px\",\"lineHeight\":\"44px\",\"fontSize\":\"inherit\",\"position\":\"absolute\"}'>用户名：</div>\r\n          <input :style='{\"border\":\"1px solid #eee\",\"padding\":\"0 10px\",\"boxShadow\":\"0 0 0px rgba(64, 158, 255, .3)\",\"outline\":\"0px solid #eee\",\"color\":\"#666\",\"outlineOffset\":\"0px\",\"borderRadius\":\"4px\",\"background\":\"none\",\"width\":\"100%\",\"fontSize\":\"inherit\",\"height\":\"40px\"}' placeholder=\"请输入用户名\" name=\"username\" type=\"text\" v-model=\"rulesForm.username\">\r\n        </div>\r\n        <div v-if=\"loginType==1\" class=\"list-item\" :style='{\"width\":\"100%\",\"margin\":\"0 0 10px\",\"position\":\"relative\",\"alignItems\":\"center\",\"flexWrap\":\"wrap\",\"display\":\"flex\"}'>\r\n          <div v-if=\"true\" class=\"lable\" :style='{\"color\":\"#333\",\"left\":\"-100px\",\"textAlign\":\"right\",\"width\":\"100px\",\"lineHeight\":\"44px\",\"fontSize\":\"inherit\",\"position\":\"absolute\"}'>密码：</div>\r\n          <input :style='{\"border\":\"1px solid #eee\",\"padding\":\"0 10px\",\"boxShadow\":\"0 0 0px rgba(64, 158, 255, .3)\",\"outline\":\"0px solid #eee\",\"color\":\"#666\",\"outlineOffset\":\"0px\",\"borderRadius\":\"4px\",\"background\":\"none\",\"width\":\"100%\",\"fontSize\":\"inherit\",\"height\":\"40px\"}' placeholder=\"请输入密码\" name=\"password\" type=\"password\" v-model=\"rulesForm.password\">\r\n        </div>\r\n\r\n        <div :style='{\"width\":\"100%\",\"margin\":\"20px auto\",\"fontSize\":\"inherit\",\"textAlign\":\"left\"}' v-if=\"roles.length>1\" prop=\"loginInRole\" class=\"list-type\">\r\n          <el-radio v-if=\"loginType==1||(loginType==2&&item.roleName!='管理员')\" v-for=\"item in roles\" v-bind:key=\"item.roleName\" v-model=\"rulesForm.role\" :label=\"item.roleName\">{{item.roleName}}</el-radio>\r\n        </div>\r\n\r\n\t\t\r\n        <div :style='{\"margin\":\"0\",\"alignItems\":\"center\",\"flexWrap\":\"wrap\",\"background\":\"none\",\"display\":\"flex\",\"width\":\"600px\",\"fontSize\":\"16px\",\"position\":\"relative\",\"justifyContent\":\"flex-start\"}'>\r\n          <el-button v-if=\"loginType==1\" :style='{\"border\":\"0px solid #4FA1D9\",\"cursor\":\"pointer\",\"padding\":\"0 20px\",\"margin\":\"0 0 16px\",\"color\":\"#fff\",\"borderRadius\":\"4px\",\"textAlign\":\"center\",\"background\":\"#2b9afc\",\"width\":\"auto\",\"letterSpacing\":\"4px\",\"fontSize\":\"16px\",\"height\":\"40px\"}' type=\"primary\" @click=\"login()\" class=\"loginInBt\">登录</el-button>\r\n        </div>\r\n      </el-form>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport menu from \"@/utils/menu\";\r\n// 引入本地图片资源\r\nimport bgImage from '@/assets/img/bg.jpg';\r\nimport loginImage from '@/assets/img/login.png';\r\n// 引入图片加载工具\r\nimport ImageLoader from '@/utils/imageLoader';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n\t\tverifyCheck2: false,\r\n\t\tflag: false,\r\n      baseUrl:this.$base.url,\r\n      loginType: 1,\r\n      rulesForm: {\r\n        username: \"\",\r\n        password: \"\",\r\n        role: \"\",\r\n      },\r\n      menus: [],\r\n      roles: [],\r\n      tableName: \"\",\r\n      bgImageLoaded: true, // 默认认为图片加载成功\r\n      formBgImageLoaded: true, // 表单背景图片加载状态\r\n      loadingProgress: 0 // 图片加载进度\r\n    };\r\n  },\r\n  computed: {\r\n    containerStyle() {\r\n      // 如果背景图片加载失败，使用渐变背景作为备用\r\n      if (this.bgImageLoaded) {\r\n        return {\r\n          \"minHeight\": \"100vh\",\r\n          \"alignItems\": \"center\",\r\n          \"background\": `url(${bgImage})`,\r\n          \"display\": \"flex\",\r\n          \"width\": \"100%\",\r\n          \"backgroundSize\": \"cover\",\r\n          \"backgroundPosition\": \"center center\",\r\n          \"backgroundRepeat\": \"no-repeat\",\r\n          \"justifyContent\": \"center\"\r\n        };\r\n      } else {\r\n        // 图片加载失败时的备用样式\r\n        return {\r\n          \"minHeight\": \"100vh\",\r\n          \"alignItems\": \"center\",\r\n          \"background\": \"linear-gradient(135deg, #667eea 0%, #764ba2 100%)\",\r\n          \"display\": \"flex\",\r\n          \"width\": \"100%\",\r\n          \"justifyContent\": \"center\"\r\n        };\r\n      }\r\n    },\r\n    formStyle() {\r\n      // 表单样式，同样提供备用方案\r\n      if (this.formBgImageLoaded) {\r\n        return {\r\n          \"border\": \"0px solid #f6f6f6\",\r\n          \"padding\": \"40px 40px 40px 500px\",\r\n          \"boxShadow\": \"-15px 15px 15px rgb(6 17 47 / 70%)\",\r\n          \"margin\": \"0 auto\",\r\n          \"alignItems\": \"flex-start\",\r\n          \"textAlign\": \"center\",\r\n          \"display\": \"flex\",\r\n          \"minHeight\": \"450px\",\r\n          \"borderRadius\": \"0\",\r\n          \"flexWrap\": \"wrap\",\r\n          \"background\": `url(${loginImage}) no-repeat left center / 420px 100%,#fff`,\r\n          \"width\": \"900px\",\r\n          \"fontSize\": \"14px\",\r\n          \"height\": \"auto\"\r\n        };\r\n      } else {\r\n        // 表单背景图片加载失败时的备用样式\r\n        return {\r\n          \"border\": \"0px solid #f6f6f6\",\r\n          \"padding\": \"40px 40px 40px 500px\",\r\n          \"boxShadow\": \"-15px 15px 15px rgb(6 17 47 / 70%)\",\r\n          \"margin\": \"0 auto\",\r\n          \"alignItems\": \"flex-start\",\r\n          \"textAlign\": \"center\",\r\n          \"display\": \"flex\",\r\n          \"minHeight\": \"450px\",\r\n          \"borderRadius\": \"0\",\r\n          \"flexWrap\": \"wrap\",\r\n          \"background\": \"#fff\",\r\n          \"width\": \"900px\",\r\n          \"fontSize\": \"14px\",\r\n          \"height\": \"auto\"\r\n        };\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    let menus = menu.list();\r\n    this.menus = menus;\r\n\r\n    for (let i = 0; i < this.menus.length; i++) {\r\n      if (this.menus[i].hasBackLogin=='是') {\r\n        this.roles.push(this.menus[i])\r\n      }\r\n    }\r\n\r\n    // 检查背景图片是否能正常加载\r\n    this.checkImageLoad();\r\n  },\r\n  created() {\r\n    // 页面创建时预加载图片\r\n    this.preloadImages();\r\n  },\r\n  destroyed() {\r\n\t    },\r\n  components: {\r\n  },\r\n  methods: {\r\n    // 预加载关键图片资源\r\n    async preloadImages() {\r\n      try {\r\n        // 预加载背景图片\r\n        await ImageLoader.preloadImage(bgImage);\r\n        this.bgImageLoaded = true;\r\n        console.log('背景图片预加载成功');\r\n      } catch (error) {\r\n        this.bgImageLoaded = false;\r\n        console.warn('背景图片预加载失败:', error.message);\r\n      }\r\n\r\n      try {\r\n        // 预加载表单背景图片\r\n        await ImageLoader.preloadImage(loginImage);\r\n        this.formBgImageLoaded = true;\r\n        console.log('表单背景图片预加载成功');\r\n      } catch (error) {\r\n        this.formBgImageLoaded = false;\r\n        console.warn('表单背景图片预加载失败:', error.message);\r\n      }\r\n    },\r\n\r\n    // 检查图片加载状态（带重试机制）\r\n    async checkImageLoad() {\r\n      try {\r\n        // 使用带重试机制的图片加载\r\n        await ImageLoader.loadImageWithRetry(bgImage, 2);\r\n        this.bgImageLoaded = true;\r\n        console.log('背景图片加载成功');\r\n      } catch (error) {\r\n        this.bgImageLoaded = false;\r\n        console.warn('背景图片加载失败，使用备用样式:', error.message);\r\n      }\r\n\r\n      try {\r\n        // 使用带重试机制的图片加载\r\n        await ImageLoader.loadImageWithRetry(loginImage, 2);\r\n        this.formBgImageLoaded = true;\r\n        console.log('表单背景图片加载成功');\r\n      } catch (error) {\r\n        this.formBgImageLoaded = false;\r\n        console.warn('表单背景图片加载失败，使用备用样式:', error.message);\r\n      }\r\n    },\r\n\r\n    //注册\r\n    register(tableName){\r\n\t\tthis.$storage.set(\"loginTable\", tableName);\r\n\t\tthis.$router.push({path:'/register',query:{pageFlag:'register'}})\r\n    },\r\n    // 登陆\r\n    login() {\r\n\r\n\t\tif (!this.rulesForm.username) {\r\n\t\t\tthis.$message.error(\"请输入用户名\");\r\n\t\t\treturn;\r\n\t\t}\r\n\t\tif (!this.rulesForm.password) {\r\n\t\t\tthis.$message.error(\"请输入密码\");\r\n\t\t\treturn;\r\n\t\t}\r\n\t\tif(this.roles.length>1) {\r\n\t\t\tif (!this.rulesForm.role) {\r\n\t\t\t\tthis.$message.error(\"请选择角色\");\r\n\t\t\t\treturn;\r\n\t\t\t}\r\n\r\n\t\t\tlet menus = this.menus;\r\n\t\t\tfor (let i = 0; i < menus.length; i++) {\r\n\t\t\t\tif (menus[i].roleName == this.rulesForm.role) {\r\n\t\t\t\t\tthis.tableName = menus[i].tableName;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t} else {\r\n\t\t\tthis.tableName = this.roles[0].tableName;\r\n\t\t\tthis.rulesForm.role = this.roles[0].roleName;\r\n\t\t}\r\n\t\t\r\n\t\tthis.loginPost()\r\n    },\r\n    loginPost() {\r\n      this.$http({\r\n        url: `${this.tableName}/login?username=${this.rulesForm.username}&password=${this.rulesForm.password}`,\r\n        method: \"post\"\r\n      }).then(({ data }) => {\r\n        if (data && data.code === 0) {\r\n          this.$storage.set(\"Token\", data.token);\r\n          this.$storage.set(\"role\", this.rulesForm.role);\r\n          this.$storage.set(\"sessionTable\", this.tableName);\r\n          this.$storage.set(\"adminName\", this.rulesForm.username);\r\n          this.$router.replace({ path: \"/\" });\r\n        } else {\r\n          this.$message.error(data.msg || '登录失败');\r\n        }\r\n      }).catch((error) => {\r\n        const msg =\r\n          (error.response && (error.response.data?.msg || error.response.data?.message)) ||\r\n          error.message ||\r\n          '网络错误';\r\n        this.$message.error(`登录请求失败：${msg}`);\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.container {\r\n  min-height: 100vh;\r\n  position: relative;\r\n  background-repeat: no-repeat;\r\n  background-position: center center;\r\n  background-size: cover;\r\n  transition: background 0.3s ease;\r\n}\r\n\r\n// 当图片加载失败时使用的备用背景\r\n.container.bg-fallback {\r\n  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\r\n}\r\n\r\n.list-item ::v-deep .el-input .el-input__inner {\r\n  border: 1px solid #eee;\r\n  border-radius: 4px;\r\n  padding: 0 10px;\r\n  box-shadow: 0 0 0px rgba(64, 158, 255, .3);\r\n  outline: 0px solid #eee;\r\n  color: #666;\r\n  background: none;\r\n  width: 100%;\r\n  font-size: inherit;\r\n  outline-offset: 0px;\r\n  height: 40px;\r\n}\r\n\r\n.list-item.select ::v-deep .el-select .el-input__inner {\r\n  border: 1px solid #eee;\r\n  border-radius: 4px;\r\n  padding: 0 10px;\r\n  margin: 0;\r\n  color: #ccc;\r\n  background: none;\r\n  width: 100%;\r\n  font-size: 14px;\r\n  height: 40px;\r\n}\r\n\r\n.list-code ::v-deep .el-input .el-input__inner {\r\n  border: 1px solid #eee;\r\n  border-radius: 4px 0 0 4px;\r\n  padding: 0 10px;\r\n  color: #666;\r\n  background: none;\r\n  width: calc(100% - 80px);\r\n  font-size: inherit;\r\n  height: 40px;\r\n}\r\n\r\n.list-type ::v-deep .el-radio__input .el-radio__inner {\r\n  background: rgba(53, 53, 53, 0);\r\n  border-color: #999;\r\n}\r\n.list-type ::v-deep .el-radio__input.is-checked .el-radio__inner {\r\n  background: #2b9afc;\r\n  border-color: #2b9afc;\r\n}\r\n.list-type ::v-deep .el-radio__label {\r\n  color: #999;\r\n  font-size: 14px;\r\n}\r\n.list-type ::v-deep .el-radio__input.is-checked+.el-radio__label {\r\n  color: #2b9afc;\r\n  font-size: 14px;\r\n}\r\n\r\n// 添加动画效果，使页面加载更平滑\r\n.container {\r\n  animation: fadeIn 0.5s ease-in-out;\r\n}\r\n\r\n@keyframes fadeIn {\r\n  from { opacity: 0; }\r\n  to { opacity: 1; }\r\n}\r\n\r\n// 确保在图片加载失败时仍有一个美观的界面\r\n.container::before {\r\n  content: \"\";\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\r\n  z-index: -2;\r\n  opacity: 0.8;\r\n}\r\n\r\n// 表单的备用背景样式\r\n.el-form {\r\n  position: relative;\r\n  z-index: 1;\r\n  transition: background 0.3s ease;\r\n}\r\n\r\n// 优化加载体验\r\n.loading-placeholder {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: linear-gradient(90deg, #f0f0f0 25%, #e0e0e0 50%, #f0f0f0 75%);\r\n  background-size: 200% 100%;\r\n  animation: loading 1.5s infinite;\r\n  z-index: -1;\r\n}\r\n\r\n@keyframes loading {\r\n  0% {\r\n    background-position: 200% 0;\r\n  }\r\n  100% {\r\n    background-position: -200% 0;\r\n  }\r\n}\r\n</style>"]}]}