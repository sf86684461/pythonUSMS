return {\r\n\t\t\tid: '',\r\n\t\t\ttype: '',\r\n\t\t\t\r\n\t\t\t\r\n\t\t\tro:{\r\n\t\t\t\tusername : false,\r\n\t\t\t\toperation : false,\r\n\t\t\t\tmethod : false,\r\n\t\t\t\tparams : false,\r\n\t\t\t\ttime : false,\r\n\t\t\t\tip : false,\r\n\t\t\t},\r\n\t\t\t\r\n\t\t\t\r\n\t\t\truleForm: {\r\n\t\t\t\tusername: '',\r\n\t\t\t\toperation: '',\r\n\t\t\t\tmethod: '',\r\n\t\t\t\tparams: '',\r\n\t\t\t\ttime: '',\r\n\t\t\t\tip: '',\r\n\t\t\t},\r\n\t\t\r\n\r\n\t\t\t\r\n\t\t\trules: {\r\n\t\t\t\tusername: [\r\n\t\t\t\t\t{ required: true, message: '用户名不能为空', trigger: 'blur' },\r\n\t\t\t\t],\r\n\t\t\t\toperation: [\r\n\t\t\t\t\t{ required: true, message: '用户操作不能为空', trigger: 'blur' },\r\n\t\t\t\t],\r\n\t\t\t\tmethod: [\r\n\t\t\t\t],\r\n\t\t\t\tparams: [\r\n\t\t\t\t],\r\n\t\t\t\ttime: [\r\n\t\t\t\t],\r\n\t\t\t\tip: [\r\n\t\t\t\t],\r\n\t\t\t}\r\n\t\t};\r\n\t},\r\n\tprops: [\"parent\"],\r\n\tcomputed: {\r\n\r\n\r\n\r\n\t},\r\n    components: {\r\n    },\r\n\tcreated() {\r\n\t},\r\n\tmethods: {\r\n\t\t\r\n\t\t// 下载\r\n\t\tdownload(file){\r\n\t\t\twindow.open(`${file}`)\r\n\t\t},\r\n\t\t// 初始化\r\n\t\tinit(id,type) {\r\n\t\t\tif (id) {\r\n\t\t\t\tthis.id = id;\r\n\t\t\t\tthis.type = type;\r\n\t\t\t}\r\n\t\t\tif(this.type=='info'||this.type=='else'){\r\n\t\t\t\tthis.info(id);\r\n\t\t\t}else if(this.type=='logistics'){\r\n\t\t\t\tthis.logistics=false;\r\n\t\t\t\tthis.info(id);\r\n\t\t\t}else if(this.type=='cross'){\r\n\t\t\t\tvar obj = this.$storage.getObj('crossObj');\r\n\t\t\t\tfor (var o in obj){\r\n\t\t\t\t\t\tif(o=='username'){\r\n\t\t\t\t\t\t\tthis.ruleForm.username = obj[o];\r\n\t\t\t\t\t\t\tthis.ro.username = true;\r\n\t\t\t\t\t\t\tcontinue;\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tif(o=='operation'){\r\n\t\t\t\t\t\t\tthis.ruleForm.operation = obj[o];\r\n\t\t\t\t\t\t\tthis.ro.operation = true;\r\n\t\t\t\t\t\t\tcontinue;\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tif(o=='method'){\r\n\t\t\t\t\t\t\tthis.ruleForm.method = obj[o];\r\n\t\t\t\t\t\t\tthis.ro.method = true;\r\n\t\t\t\t\t\t\tcontinue;\r\n