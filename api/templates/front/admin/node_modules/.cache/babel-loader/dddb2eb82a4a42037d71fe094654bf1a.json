rm-item>\r\n\t\t\t\t<el-form-item :style='{\"border\":\"0px solid #eee\",\"padding\":\"0px 0\",\"margin\":\"0 0 20px 0\",\"color\":\"inherit\",\"borderRadius\":\"0px\",\"background\":\"none\",\"width\":\"100%\",\"fontSize\":\"inherit\"}' class=\"input\" v-if=\"type!='info'\"  label=\"用户操作\" prop=\"operation\">\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.operation\" placeholder=\"用户操作\" clearable  :readonly=\"ro.operation\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item :style='{\"border\":\"0px solid #eee\",\"padding\":\"0px 0\",\"margin\":\"0 0 20px 0\",\"color\":\"inherit\",\"borderRadius\":\"0px\",\"background\":\"none\",\"width\":\"100%\",\"fontSize\":\"inherit\"}' v-else class=\"input\" label=\"用户操作\" prop=\"operation\">\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.operation\" placeholder=\"用户操作\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item :style='{\"border\":\"0px solid #eee\",\"padding\":\"0px 0\",\"margin\":\"0 0 20px 0\",\"color\":\"inherit\",\"borderRadius\":\"0px\",\"background\":\"none\",\"width\":\"100%\",\"fontSize\":\"inherit\"}' class=\"input\" v-if=\"type!='info'\"  label=\"请求方法\" prop=\"method\">\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.method\" placeholder=\"请求方法\" clearable  :readonly=\"ro.method\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item :style='{\"border\":\"0px solid #eee\",\"padding\":\"0px 0\",\"margin\":\"0 0 20px 0\",\"color\":\"inherit\",\"borderRadius\":\"0px\",\"background\":\"none\",\"width\":\"100%\",\"fontSize\":\"inherit\"}' v-else class=\"input\" label=\"请求方法\" prop=\"method\">\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.method\" placeholder=\"请求方法\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item :style='{\"border\":\"0px solid #eee\",\"padding\":\"0px 0\",\"margin\":\"0 0 20px 0\",\"color\":\"inherit\",\"borderRadius\":\"0px\",\"background\":\"none\",\"width\":\"100%\",\"fontSize\":\"inherit\"}' class=\"input\" v-if=\"type!='info'\"  label=\"