/div>\r\n\t    <div :style='{\"width\":\"auto\",\"display\":\"inline-block\",\"height\":\"auto\"}' class=\"item-body\">\r\n\t      <div class=\"item\" @click=\"selectClick2(item,-1)\" :class=\"item.check ==-1 ? 'active' : ''\">全部</div>\r\n\t      <div class=\"item\" @click=\"selectClick2(item,index1)\" :class=\"item.check == index1 ? 'active' : ''\" v-for=\"item1,index1 in item.list\" :key=\"item1\">{{item1}}</div>\r\n\t    </div>\r\n\t  </div>\r\n\t</div>\r\n\t<div class=\"sort_view\" :style='{\"padding\":\"0 2%\",\"margin\":\"20px 0 0\",\"overflow\":\"hidden\",\"textAlign\":\"right\",\"background\":\"none\",\"width\":\"100%\",\"order\":\"4\"}'>\r\n\t\t<el-button :style='{\"padding\":\"2px 2px\",\"margin\":\"0 5px\",\"borderColor\":\"#eccc19\",\"borderRadius\":\"0px\",\"background\":\"none\",\"borderWidth\":\"0px\",\"borderStyle\":\"solid\"}' @click=\"sortClick('price')\">\r\n\t\t\t<span :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"20px\",\"fontSize\":\"14px\",\"color\":\"#ca1913\"}' class=\"icon iconfont icon-chujia13\" v-if=\"sortType!='price'\"></span>\r\n\t\t\t<span :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"20px\",\"fontSize\":\"14px\",\"color\":\"#ca1913\"}' class=\"icon iconfont icon-jiantou23\" v-else-if=\"sortType=='price'&&sortOrder=='desc'\"></span>\r\n\t\t\t<span :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"20px\",\"fontSize\":\"14px\",\"color\":\"#ca1913\"}' class=\"icon iconfont icon-jiantou24\" v-else-if=\"sortType=='price'&&sortOrder=='asc'\"></span>\r\n\t\t\t<span :style='{\"color\":\"#333\",\"lineHeight\":\"20px\",\"fontSize\":\"14px\"}'>价格</span>\r\n\t\t</el-button>\r\n\t\t<el-button :style='{\"border\":\"0px solid #ddd\",\"padding\":\"2px 2px\",\"margin\":\"0 5px\",\"borderRadius\":\"0px\",\"background\":\"none\"}' @click=\"sortClick('clicknum')\">\r\n\t\t\t<span :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"20px\",\"fontSize\":\"14px\",\"color\":\"#ca1913\"}' class=\"icon iconfont icon-xiaoliang17\" v-if=\"sortType!='clicknum'\"></span>\r\n\t\t\t<span :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"20px\",\"fontSize\":\"14px\",\"color\":\"#ca1913\"}' class=\"icon iconfont icon-jiantou23\" v-else-if=\"sortType=='clicknum'&&sortOrder=='desc'\"></span>\r\n\t\t\t<span :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"20px\",\"fontSize\":\"14px\",\"color\":\"#ca1913\"}' class=\"icon iconfont icon-jiantou24\" v-else-if=\"sortType=='clicknum'&&sortOrder=='asc'\"></span>\r\n\t\t\t<span :style='{\"color\":\"#333\",\"lineHeight\":\"20px\",\"fontSize\":\"14px\"}'>点击量</span>\r\n\t\t</el-button>\r\n\t\t<el-button :style='{\"border\":\"0\",\"padding\":\"2px 2px\",\"margin\":\"0 5px\",\"borderRadius\":\"8px\"}' @click=\"sortClick('storeupnum')\">\r\n\t\t\t<span :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"40px\",\"fontSize\":\"14px\",\"color\":\"#ca1913\"}' class=\"icon iconfont icon-xiaoliang13\" v-if=\"sortType!='storeupnum'\"></span>\r\n\t\t\t<span :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"40px\",\"fontSize\":\"14px\",\"color\":\"#ca1913\"}' class=\"icon iconfont icon-jiantou23\" v-else-if=\"sortType=='storeupnum'&&sortOrder=='desc'\"></span>\r\n\t\t\t<span :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"40px\",\"fontSize\":\"14px\",\"color\":\"#ca1913\"}' class=\"icon iconfont icon-jiantou24\" v-else-if=\"sortType=='storeupnum'&&sortOrder=='asc'\"></span>\r\n\t\t\t<span :style='{\"color\":\"#333\",\"lineHeight\":\"40px\",\"fontSize\":\"14px\"}'>收藏数</span>\r\n\t\t</el-button>\r\n\t</div>\r\n\t<div class=\"list\" :style='{\"minHeight\":\"450px\",\"padding\":\"20px 0 0 0\",\"margin\":\"0\",\"background\":\"none\",\"flex\":\"1\",\"width\":\"calc(100% - 250px)\",\"order\":\"6\"}'>\r\n\t\t<!-- 样式一 -->\r\n\t\t<div class=\"list1 index-pv1\" :style='{\"padding\":\"0 0px\",\"color\":\"#666\",\"flexWrap\":\"wrap\",\"background\":\"none\",\"display\":\"flex\",\"width\":\"100%\",\"fontSize\":\"14px\",\"height\":\"auto\"}'>\r\n\t\t\t<div :style='{\"border\":\"0px solid #ddd\",\"cursor\":\"pointer\",\"padding\":\"0 0 10px\",\"margin\":\"0 2% 20px 0\",\"overflow\":\"hidden\",\"flexWrap\":\"wrap\",\"background\":\"#fff\",\"display\":\"flex\",\"width\":\"18%\",\"position\":\"relative\",\"height\":\"auto\"}' v-for=\"(item, index) in dataList\" :key=\"index\" @click.stop=\"toDetail(item)\" class=\"list-item animation-box\">\r\n\t\t\t\t<img @click.stop=\"imgPreView(item.shangpintupian)\" :style='{\"border\":\"1px solid #ddd\",\"width\":\"100%\",\"padding\":\"6px\",\"objectFit\":\"contain\",\"display\":\"block\",\"height\":\"240px\"}' v-if=\"item.shangpintupian && item.shangpintupian.substr(0,4)=='http'\" :src=\"item.shangpintupian.split(',')[0]\" class=\"image\" />\r\n\t\t\t\t<img @click.stop=\"imgPreView(baseUrl + (item.shangpintupian?item.shangpintupian.split(',')[0]:''))\" :style='{\"border\":\"1px solid #ddd\",\"width\":\"100%\",\"padding\":\"6px\",\"objectFit\":\"contain\",\"display\":\"block\",\"height\":\"240px\"}' v-else :src=\"baseUrl + (item.shangpintupian?item.shangpintupian.split(',')[0]:'')\" class=\"image\" />\r\n\t\t\t\t<div v-if=\"item.price\" :style='{\"padding\":\"0 10px\",\"color\":\"#c00\",\"textAlign\":\"left\",\"width\":\"100%\",\"lineHeight\":\"24px\",\"fontSize\":\"inherit\",\"order\":\"2\"}' class=\"price\"><span :style='{\"fontSize\":\"12px\"}'>￥</span>{{item.price}}</div>\r\n\t\t\t\t<div :style='{\"padding\":\"0 10px\",\"borderColor\":\"#ddd\",\"margin\":\"0 0 6px\",\"overflow\":\"hidden\",\"whiteSpace\":\"nowrap\",\"color\":\"#333\",\"borderWidth\":\"0 0 0px\",\"width\":\"100%\",\"lineHeight\":\"24px\",\"fontSize\":\"14px\",\"textOverflow\":\"ellipsis\",\"borderStyle\":\"solid\"}' class=\"name \">{{item.shangpinmingcheng}}</div>\r\n\t\t\t\t<div :style='{\"padding\":\"0 10px\",\"borderColor\":\"#ddd\",\"margin\":\"0 0 6px\",\"overflow\":\"hidden\",\"whiteSpace\":\"nowrap\",\"color\":\"#333\",\"borderWidth\":\"0 0 0px\",\"width\":\"100%\",\"lineHeight\":\"24px\",\"fontSize\":\"14px\",\"textOverflow\":\"ellipsis\",\"borderStyle\":\"solid\"}' class=\"name \">{{item.gongyingshang}}</div>\r\n\t\t\t\t<div :style='{\"width\":\"100%\",\"padding\":\"0 10px\",\"textAlign\":\"left\",\"display\":\"none\",\"order\":\"3\"}'>\r\n\t\t\t\t  <span class=\"icon iconfont icon-shijian21\" :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"24px\",\"fontSize\":\"inherit\",\"color\":\"inherit\"}'></span>\r\n\t\t\t\t  <span class=\"text\" :style='{\"color\":\"#999\",\"lineHeight\":\"32px\",\"fontSize\":\"inherit\"}'>{{item.addtime}}</span>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div :style='{\"padding\":\"0 10px\",\"display\":\"inline-block\",\"order\":\"6\"}'>\r\n\t\t\t\t  <span class=\"icon iconfont icon-shoucang10\" :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\",\"color\":\"inherit\"}'></span>\r\n\t\t\t\t  <span class=\"text\" :style='{\"color\":\"inherit\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\"}'>{{item.storeupnum}}</span>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div :style='{\"padding\":\"0 10px\",\"display\":\"inline-block\",\"order\":\"7\"}' v-if=\"item.clicknum\">\r\n\t\t\t\t  <span class=\"icon iconfont icon-chakan2\" :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\",\"color\":\"inherit\"}'></span>\r\n\t\t\t\t  <span class=\"text\" :style='{\"color\":\"inherit\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\"}'>{{item.clicknum}}</span>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t\t\r\n\t</div>\r\n\r\n\t\r\n\t<el-pagination\r\n\t  background\r\n\t  id=\"pagination\"\r\n\t  class=\"pagination\"\r\n\t  :pager-count=\"7\"\r\n\t  :page-size=\"pageSize\"\r\n\t  prev-text=\"上一页\"\r\n\t  next-text=\"下一页\"\r\n\t  :hide-on-single-page=\"false\"\r\n\t  :layout='[\"total\",\"prev\",\"pager\",\"next\",\"sizes\"].join()'\r\n\t  :total=\"total\"\r\n\t  :style='{\"padding\":\"0\",\"margin\":\"10px auto\",\"color\":\"#333\",\"textAlign\":\"center\",\"width\":\"100%\",\"lineHeight\":\"40px\",\"float\":\"left\",\"fontWeight\":\"500\",\"height\":\"40px\",\"o