","init","$config","getCurDate","methods","getMakeZero","s","download","file","window","open","obj","JSON","parse","o","info","$http","emulateJSON","then","res","code","json","removeItem","setTimeout","onSubmit","String","$message","error","table","parseFloat","crossuserid","crossrefid","crossoptnum","$refs","validate","valid","statusColumnName","statusColumnValue","startsWith","post","Number","replace","params","page","limit","total","duration","onClose","$router","go","msg","Date","getTime","back"],"sources":["src/pages/shangpinjinhuo/add.vue"],"sourcesContent":["<template>\r\n<div :style='{\"width\":\"100%\",\"padding\":\"30px calc((100% - 1200px)/2) 40px\",\"margin\":\"0px auto\",\"position\":\"relative\",\"background\":\"#fff\"}'>\r\n    <el-form\r\n\t  :style='{\"border\":\"1px solid #eee\",\"padding\":\"30px 0\",\"alignItems\":\"flex-start\",\"flexWrap\":\"wrap\",\"background\":\"#fff\",\"display\":\"flex\",\"width\":\"100%\",\"position\":\"relative\"}'\r\n      class=\"add-update-preview\"\r\n      ref=\"ruleForm\"\r\n      :model=\"ruleForm\"\r\n      :rules=\"rules\"\r\n      label-width=\"120px\"\r\n    >\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"入库单号\" prop=\"rukudanhao\">\r\n              <el-input v-model=\"ruleForm.rukudanhao\" placeholder=\"入库单号\" disabled></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"商品名称\" prop=\"shangpinmingcheng\">\r\n            <el-input v-model=\"ruleForm.shangpinmingcheng\" \r\n                placeholder=\"商品名称\" clearable :disabled=\" false  ||ro.shangpinmingcheng\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"商品分类\" prop=\"shangpinfenlei\">\r\n            <el-input v-model=\"ruleForm.shangpinfenlei\" \r\n                placeholder=\"商品分类\" clearable :disabled=\" false  ||ro.shangpinfenlei\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"进货数量\" prop=\"alllimittimes\">\r\n            <el-input v-model.number=\"ruleForm.alllimittimes\" \r\n                placeholder=\"进货数量\" clearable :disabled=\" false  ||ro.alllimittimes\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"入库价格\" prop=\"rukujiage\">\r\n            <el-input v-model.number=\"ruleForm.rukujiage\" \r\n                placeholder=\"入库价格\" clearable :disabled=\" false  ||ro.rukujiage\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"入库总价\" prop=\"rukuzongjia\">\r\n              <el-input v-model=\"rukuzongjia\" placeholder=\"入库总价\" disabled></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"供应商\" prop=\"gongyingshang\">\r\n            <el-input v-model=\"ruleForm.gongyingshang\" \r\n                placeholder=\"供应商\" clearable :disabled=\" false  ||ro.gongyingshang\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"入库时间\" prop=\"rukushijian\">\r\n              <el-date-picker\r\n\t\t\t\t  :disabled=\" false  ||ro.rukushijian\"\r\n                  format=\"yyyy 年 MM 月 dd 日\"\r\n                  value-format=\"yyyy-MM-dd\"\r\n                  v-model=\"ruleForm.rukushijian\" \r\n                  type=\"date\"\r\n                  placeholder=\"入库时间\">\r\n              </el-date-picker> \r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"员工工号\" prop=\"yuangonggonghao\">\r\n            <el-input v-model=\"ruleForm.yuangonggonghao\" \r\n                placeholder=\"员工工号\" clearable :disabled=\" false  ||ro.yuangonggonghao\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"员工姓名\" prop=\"yuangongxingming\">\r\n            <el-input v-model=\"ruleForm.yuangongxingming\" \r\n                placeholder=\"员工姓名\" clearable :disabled=\" false  ||ro.yuangongxingming\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"备注\" prop=\"beizhu\">\r\n            <el-input v-model=\"ruleForm.beizhu\" \r\n                placeholder=\"备注\" clearable :disabled=\" false  ||ro.beizhu\"></el-input>\r\n          </el-form-item>\r\n\r\n      <el-form-item :style='{\"width\":\"100%\",\"padding\":\"0\",\"margin\":\"0\",\"textAlign\":\"center\"}'>\r\n        <el-button :style='{\"border\":\"0px solid #eccc19\",\"cursor\":\"pointer\",\"padding\":\"0\",\"margin\":\"0 20px 0 0\",\"color\":\"#fff\",\"borderRadius\":\"20px\",\"background\":\"linear-gradient(to right,#f67536,#f64d36),#f64d36\",\"width\":\"128px\",\"lineHeight\":\"40px\",\"fontSize\":\"14px\",\"height\":\"40px\"}'  type=\"primary\" @click=\"onSubmit\">提交</el-button>\r\n        <el-button :style='{\"border\":\"1px solid #ccc\",\"cursor\":\"pointer\",\"padding\":\"0\",\"margin\":\"0\",\"outline\":\"none\",\"color\":\"#666\",\"borderRadius\":\"20px\",\"background\":\"none\",\"width\":\"128px\",\"lineHeight\":\"40px\",\"fontSize\":\"14px\",\"height\":\"40px\"}' @click=\"back()\">返回</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    data() {\r\n\t  let self = this\r\n      return {\r\n        id: '',\r\n        baseUrl: '',\r\n        ro:{\r\n\t\t\t\trukudanhao : false,\r\n\t\t\t\tshangpinmingcheng : false,\r\n\t\t\t\tshangpinfenlei : false,\r\n\t\t\t\talllimittimes : false,\r\n\t\t\t\trukujiage : false,\r\n\t\t\t\trukuzongjia : false,\r\n\t\t\t\tgongyingshang : false,\r\n\t\t\t\trukushijian : false,\r\n\t\t\t\tyuangonggonghao : false,\r\n\t\t\t\tyuangongxingming : false,\r\n\t\t\t\tbeizhu : false,\r\n        },\r\n        type: '',\r\n        userTableName: localStorage.getItem('UserTableName'),\r\n        ruleForm: {\r\n          rukudanhao: this.getUUID(),\r\n          shangpinmingcheng: '',\r\n          shangpinfenlei: '',\r\n          alllimittimes: '',\r\n          rukujiage: '',\r\n          rukuzongjia: '',\r\n          gongyingshang: '',\r\n          rukushijian: '',\r\n          yuangonggonghao: '',\r\n          yuangongxingming: '',\r\n          beizhu: '',\r\n        },\r\n\r\n\r\n        rules: {\r\n          rukudanhao: [\r\n          ],\r\n          shangpinmingcheng: [\r\n          ],\r\n          shangpinfenlei: [\r\n          ],\r\n          alllimittimes: [\r\n            { validator: this.$validate.isIntNumer, trigger: 'blur' },\r\n          ],\r\n          rukujiage: [\r\n            { required: true, message: '入库价格不能为空', trigger: 'blur' },\r\n            { validator: this.$validate.isIntNumer, trigger: 'blur' },\r\n          ],\r\n          rukuzongjia: [\r\n            { validator: this.$validate.isNumber, trigger: 'blur' },\r\n          ],\r\n          gongyingshang: [\r\n          ],\r\n          rukushijian: [\r\n          ],\r\n          yuangonggonghao: [\r\n          ],\r\n          yuangongxingming: [\r\n          ],\r\n          beizhu: [\r\n          ],\r\n        },\r\n\t\tcenterType: false,\r\n      };\r\n    },\r\n    computed: {\r\n\r\n\r\n        rukuzongjia:{\r\n            get: function () {\r\n                return 1*this.ruleForm.alllimittimes*this.ruleForm.rukujiage\r\n            }\r\n        },\r\n\r\n    },\r\n    components: {\r\n    },\r\n    created() {\r\n\t\tif(this.$route.query.centerType){\r\n\t\t\tthis.centerType = true\r\n\t\t}\r\n\t  //this.bg();\r\n      let type = this.$route.query.type ? this.$route.query.type : '';\r\n      this.init(type);\r\n      this.baseUrl = this.$config.baseUrl;\r\n      this.ruleForm.rukushijian = this.getCurDate()\r\n    },\r\n    methods: {\r\n      getMakeZero(s) {\r\n          return s < 10 ? '0' + s : s;\r\n      },\r\n      // 下载\r\n      download(file){\r\n        window.open(`${file}`)\r\n      },\r\n      // 初始化\r\n      init(type) {\r\n        this.type = type;\r\n        if(type=='cross'){\r\n          var obj = JSON.parse(localStorage.getItem('crossObj'));\r\n          for (var o in obj){\r\n            if(o=='rukudanhao'){\r\n              this.ruleForm.rukudanhao = obj[o];\r\n              this.ro.rukudanhao = true;\r\n              continue;\r\n            }\r\n            if(o=='shangpinmingcheng'){\r\n              this.ruleForm.shangpinmingcheng = obj[o];\r\n              this.ro.shangpinmingcheng = true;\r\n              continue;\r\n            }\r\n            if(o=='shangpinfenlei'){\r\n              this.ruleForm.shangpinfenlei = obj[o];\r\n              this.ro.shangpinfenlei = true;\r\n              continue;\r\n            }\r\n            if(o=='alllimittimes'){\r\n              this.ruleForm.alllimittimes = obj[o];\r\n              this.ro.alllimittimes = true;\r\n              continue;\r\n            }\r\n            if(o=='rukujiage'){\r\n              this.ruleForm.rukujiage = obj[o];\r\n              this.ro.rukujiage = true;\r\n              continue;\r\n            }\r\n            if(o=='rukuzongjia'){\r\n              this.ruleForm.rukuzongjia = obj[o];\r\n              this.ro.rukuzongjia = true;\r\n              continue;\r\n            }\r\n            if(o=='gongyingshang'){\r\n              this.ruleForm.gongyingshang = obj[o];\r\n              this.ro.gongyingshang = true;\r\n              continue;\r\n            }\r\n            if(o=='rukushijian'){\r\n              this.ruleForm.rukushijian = obj[o];\r\n              this.ro.rukushijian = true;\r\n              continue;\r\n            }\r\n            if(o=='yuangonggonghao'){\r\n              this.ruleForm.yuangonggonghao = obj[o];\r\n              this.ro.yuangonggonghao = true;\r\n              continue;\r\n            }\r\n            if(o=='yuangongxingming'){\r\n              this.ruleForm.yuangongxingming = obj[o];\r\n              this.ro.yuangongxingming = true;\r\n              continue;\r\n            }\r\n            if(o=='beizhu'){\r\n              this.ruleForm.beizhu = obj[o];\r\n              this.ro.beizhu = true;\r\n              continue;\r\n            }\r\n          }\r\n            this.ruleForm.alllimittimes = 0;\r\n            this.ro.alllimittimes = false;\r\n        }else if(type=='edit'){\r\n\t\t\tthis.info()\r\n\t\t}\r\n        // 获取用户信息\r\n        this.$http.get(this.userTableName + '/session', {emulateJSON: true}).then(res => {\r\n          if (res.data.code == 0) {\r\n            var json = res.data.data;\r\n            if((json.yuangonggonghao!=''&&json.yuangonggonghao) || json.yuangonggonghao==0){\r\n                this.ruleForm.yuangonggonghao = json.yuangonggonghao;\r\n\t\t\t\tthis.ro.yuangonggonghao = true;\r\n            }\r\n            if((json.yuangongxingming!=''&&json.yuangongxingming) || json.yuangongxingming==0){\r\n                this.ruleForm.yuangongxingming = json.yuangongxingming;\r\n\t\t\t\tthis.ro.yu